<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mykotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.ViewActivity"/>
        <activity
            android:name=".material.WidgetActivity"
            android:theme="@style/MaterialComponents" />
        <activity android:name=".sunnyweather.ui.weather.WeatherActivity" />
        <activity android:name=".sunnyweather.ui.MainWeatherActivity" />
        <activity
            android:name=".material.FruitActivity"
            android:theme="@style/TransparentStatusBar" />
        <activity
            android:name=".material.MaterialActivity"
            android:label="Fruits" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".result.OneActivity" />
        <activity android:name=".result.TwoActivity" />
        <activity android:name=".result.ThreeActivity" />
    </application>

</manifest>